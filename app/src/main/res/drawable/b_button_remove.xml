<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item >
        <shape android:shape="rectangle">

            <padding android:left="8.5px"
                android:top="8.5px"
                android:right="8.5px"
                android:bottom="8.5px"/>
            <corners android:radius="5dp" />
            <gradient
                android:type="radial"
                android:gradientRadius="50dp"
                android:centerColor="#beb98283"
                android:endColor="#becc7879" />

        </shape>
    </item>

    <item >
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <gradient
                android:type="radial"
                android:gradientRadius="100dp"
                android:startColor="#c68b8c"
                android:centerColor="#bf6062"
                android:endColor="#ac484a" />


        </shape>
    </item>


</layer-list>