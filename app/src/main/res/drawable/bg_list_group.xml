<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item >
        <shape android:shape="rectangle">

            <padding android:left="8.5px"
                android:top="8.5px"
                android:right="8.5px"
                android:bottom="8.5px"/>
            <corners android:radius="5dp" />
            <gradient
                android:type="radial"
                android:gradientRadius="1000dp"
                android:centerColor="#ff911b"
                android:endColor="#ff8400" />

        </shape>
    </item>

    <item >
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <gradient
                android:type="radial"
                android:gradientRadius="1000dp"
                android:startColor="#ff9829"
                android:centerColor="#fd8f18"
                android:endColor="#ff8400" />


        </shape>
    </item>


</layer-list>